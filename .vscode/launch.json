  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "codeceptjs",
      "program": "${workspaceFolder}/node_modules/codeceptjs/bin/codecept.js",
      "args": [
        "run",
        "--grep",
        "@pricing",
        "--debug",
        "--config",
        "codecept.conf.js"
      ],
      "outputCapture": "std",
      "skipFiles": ["node_modules/**/*.js"]
    }
  ]
}
